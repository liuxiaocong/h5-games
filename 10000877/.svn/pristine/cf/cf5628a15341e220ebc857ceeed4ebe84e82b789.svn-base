var startpos=0;
var endpos=0;
var isload=false;
function draw(){
	var sr="";
	
	var backtop=document.getElementById('backtop');
	var loading=document.getElementById('loading');
	if(endpos!=games.length){
		endpos+=20;
		if(backtop){
			backtop.style.display="none";
		}
	}else{
		if(backtop){
			backtop.style.display="block";
		}
		if(loading){
			
			loading.style.display="none";
		}
	}
	if(endpos>games.length){
		endpos=games.length;
	}
	var gamelist=document.getElementById("j_game_list");
	for(var i=startpos;i<endpos;i++){
		var gtype=games[i].type;
		if(!gtype)gtype="休闲竞技";
		var score=games[i].score;
		if(!score)score=60;		
		var li=document.createElement("li");
		li.setAttribute("data-url",games[i].src);
		li.innerHTML="<figure><img src='" + games[i].icon + "'/><figcaption><h3 class='tit'>" + games[i].name + "</h3><p>"+gtype+" 好评" + score + "%</p><p><span>" + (100000+parseInt(Math.random()*1000000)) + "人玩过</span></p></figcaption><span class='btn btn-g' data-url='" + games[i].src + "'>开始玩</span></figure>";
		gamelist.appendChild(li);
	}
	startpos=endpos;
	isload=true;
	//if(t==1)document.getElementById("menu").innerHTML='<a href="/?ver=0.3&' +  ((Game.request("uid"))?("uid="+Game.request("uid")):"")  + '" class="nav_firstItem"><span class="common_icon_favor"></span>即点即玩</a><a href="/?t=2&ver=0.3&' +  ((Game.request("uid"))?("uid="+Game.request("uid")):"")  + '" class="nav_lastItem"><span class="common_icon_catalog" ></span>热门游戏下载</a>';
	//if(t==2)document.getElementById("menu").innerHTML='<a href="/?ver=0.3&' +  ((Game.request("uid"))?("uid="+Game.request("uid")):"")  + '" class="nav_lastItem"><span class="common_icon_favor"></span>即点即玩</a><a href="/?t=2&ver=0.3&' +  ((Game.request("uid"))?("uid="+Game.request("uid")):"")  + '" class="nav_firstItem"><span class="common_icon_catalog" ></span>热门游戏下载</a>';

}
window.addEventListener ("scroll",function(e){
	
	var sctop=document.body.scrollTop;
	var footnav=document.getElementById("j_foot_nav");
	if(!footnav){
		return;
	}
	var btop=footnav.offsetTop;
	if(btop-sctop<1000){
		if(isload){
			
			isload=false;
			draw();
			
		}
	}
})

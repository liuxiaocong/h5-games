.tgs-blur{-webkit-filter:blur(12px);-moz-filter:blur(12px);-o-filter:blur(12px);-ms-filter:blur(12px);filter:blur(12px)}.tgs-ad-frame{background:#fff;padding:25px;-webkit-border-radius:10px;border-radius:10px;min-width:300px;min-height:250px}.tgs-ad-frame.fullscreen{width:100%!important;height:100%!important;-webkit-border-radius:0;border-radius:0;padding:0}.tgs-ad-frame.fullscreen span{display:none}.tgs-ad-frame span{position:absolute;width:100%;height:25px;line-height:25px;text-align:center;bottom:0;left:0;color:#666;font-size:12px}#tgs-leaderboard-container{width:100%;height:100%;position:absolute;z-index:1000}#tgs-leaderboard-container iframe{width:100%;height:100%;border:0;overflow:hidden}.tgs-btn{-moz-box-sizing:border-box;-webkit-box-sizing:border-box;box-sizing:border-box;overflow:hidden;margin-bottom:10px;height:55px;line-height:55px;background-color:#111;color:#eee;font-size:18px;text-align:center;width:100%;cursor:pointer}.tgs-btn span{white-space:nowrap}.tgs-btn:hover{background-color:#2b2b2b}.tgs-btn.fb{background-color:#3b5999}.tgs-btn.fb:hover{background-color:#4d70ba}.tgs-btn.twitter{background-color:#55acee}.tgs-btn.twitter:hover{background-color:#83c3f3}.tgs-btn.kik{background-color:#6dbb1a}.tgs-btn.kik:hover{background-color:#87e127}#tgs-widget{position:absolute}#tgs-widget .btn-container{float:left;margin-bottom:-10px;width:100%}#tgs-widget .tgs-btn img{float:left;height:100%;margin-left:8px}#tgs-widget #tgs_display_ad{width:300px;height:250px;margin-bottom:10px}#tgs-widget.horizontal #tgs_display_ad{margin-bottom:0;margin-right:10px;float:left}#tgs-widget.two-column .tgs-btn{float:left}#tgs-widget.four-column .tgs-btn{float:left;margin-bottom:0}#tgs-widget.four-column .btn-container{margin-bottom:0}#tgs-widget.four-column .tgs-btn:last-child,#tgs-widget.four-column .tgs-btn:nth-child(4n){margin-right:0!important}#tgs-widget.login .tgs-btn{height:auto;background:0 0;margin-bottom:0}#tgs-widget.login .tgs-btn span{display:none}#tgs-widget.login .tgs-btn img{width:100%;height:auto;float:none;margin-left:0}